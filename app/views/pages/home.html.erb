<body class= "banner">
<div class="photo-card">
  <%= image_tag "background.jpg", autoplay: :autoplay, loop: :loop, muted: :muted, class: "home_photo" %>
  <div class= "container-fluid">
    <h2>Welcome to SurfBnB!</h2>
    <p><%= link_to "Explore Surfboards", surfboards_path %></p>
    <% unless current_user.nil? %>
    <p><%= link_to "My Surfboards", dashboard_path(current_user) %></p>
    <% end %>
  </div>
</div>
</body>
